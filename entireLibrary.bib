  month         = oct,
  title         = {Identification of port-{H}amiltonian systems from frequency response data},
  year          = {2019}
}

@article{Aoues2019,
  abstract  = {In this brief, we develop a mathematical model of a flexible spacecraft system composed of a hub and two symmetrical beams using the port-{H}amiltonian framework. This class of system has favorable properties, such as passivity for controller synthesis and stability analysis, where the global {H}amiltonian plays the role of a Lyapunov function candidate. The spacecraft model is viewed as a power-conserving interconnection between an infinite (beam) and finite (hub) dimensional system. We show that the interconnection result has a port-{H}amiltonian structure and is passive. The introduction of a nonlinear feedback term, which takes into account the beam's flexibility, is developed using the control by an interconnection approach. The closedloop stability is proven; then, through explicitly solving the partial differential equations of the system, asymptotic stability is obtained. Finally, the experimental results are carried out to assess the validity of the proposed design methodology.},
  author    = {Aoues, Said and Cardoso-Ribeiro, Flavio Luiz and Matignon, Denis and Alazard, Daniel},
  doi       = {10.1109/tcst.2017.2771244},
  file      = {:AouesCardoso-RibeiroMatignonAlazard/Aoues2019 - Modeling and Control of a Rotating Flexible Spacecraft_ a Port Hamiltonian Approach.pdf:PDF},
  groups    = {Geometric Mechanics},
  journal   = {{IEEE} Transactions on Control Systems Technology},
  keywords  = {Flexible spacecraft, Lyapunov stability, mixed port-{H}amiltonian systems (PHSs), passivity-based control},
  month     = jan,
  number    = {1},
  pages     = {355--362},
  publisher = {Institute of Electrical and Electronics Engineers ({IEEE})},
  title     = {Modeling and control of a rotating flexible spacecraft: a port-{H}amiltonian approach},
  volume    = {27},
  year      = {2019}
}

