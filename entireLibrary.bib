  groups    = {Geometric Mechanics},
  journal   = {{IMA} Journal of Mathematical Control and Information},
  keywords  = {port-{H}amiltonian theory, distributed parameter systems, multi-physical systems, energybased control, spatial discretization},
  month     = jul,
