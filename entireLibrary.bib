@InProceedings{Bruder2019,
  author    = {Bruder, Daniel and Gillespie, Brent and Remy, C. David and Vasudevan, Ram},
  booktitle = {Robotics: Science and Systems {XV}},
  title     = {Modeling and control of soft robots using the koopman operator and model predictive control},
  year      = {2019},
  month     = jun,
  publisher = {Robotics: Science and Systems Foundation},
  abstract  = {Controlling soft robots with precision is a challenge due in large part to the difficulty of constructing models that are amenable to model-based control design techniques. Koopman operator theory offers a way to construct explicit linear dynamical models of soft robots and to control them using established model-based linear control methods. This method is data-driven, yet unlike other data-driven models such as neural networks, it yields an explicit control-oriented linear model rather than just a "black-box" input-output mapping. This work describes this Koopman-based system identification method and its application to model predictive controller design. A model and MPC controller of a pneumatic soft robot arm is constructed via the method, and its performance is evaluated over several trajectory following tasks in the real-world. On all of the tasks, the Koopman-based MPC controller outperforms a benchmark MPC controller based on a linear state-space model of the same system.},
  doi       = {10.15607/rss.2019.xv.060},
  file      = {:prio2/BruderGillespieRemyVasudevan/Bruder2019 - Modeling and Control of Soft Robots Using the Koopman Operator and Model Predictive Control.pdf:PDF},
  groups    = {Soft Robotics},
  priority  = {prio2},
}

@article{Baecher2021,
  abstract  = {Purpose of Review We discuss the use of differentiable simulation for computational problems in soft robotics. This includes characterizing the mechanical behavior of soft robots, optimally controlling embedded soft actuators or active materials, and estimating the robot's state from readings of embedded sensors. Moreover, we discuss how design optimization can help to optimally place soft actuators and sensors. Recent Findings We expatiate on the adoption of simulation and optimization tools in the process of designing and controlling soft robots. We include a discussion of rigid-flexible systems and the use of differentiable simulation in combination with machine learning. Summary We review the state of the art in the computational modeling of soft robots and provide a summary of the required mathematical tools. We also review several open questions where computation could help to move the field forward, and discuss the role of differentiable simulation in managing the ever-growing design complexity of nextgeneration soft robots.},
  author    = {Bächer, Moritz and Knoop, Espen and Schumacher, Christian},
  date      = {2021-05-10},
  day       = {10},
  doi       = {10.1007/s43154-021-00052-7},
  file      = {:BächerKnoopSchumacher/Baecher2021 - Design and Control of Soft Robots Using Differentiable Simulation.pdf:PDF},
  groups    = {Soft Robotics},
  journal   = {Current Robotics Reports},
  keywords  = {Differentiable Simulation, Computational Design and Control, State Estimation, Sensor and Actuator Design},
  month     = may,
  number    = {2},
  pages     = {211--221},
  publisher = {Springer Science and Business Media {LLC}},
  title     = {Design and control of soft robots using differentiable simulation},
  volume    = {2},
  year      = {2021}
}

@Article{Allard2007,
  author       = {Allard, J. and Cotin, S. and Faure, F. and Bensoussan, P.-J. and Poyer, F. and Duriez, C. and Delingette, H. and Grisoni, L.},
