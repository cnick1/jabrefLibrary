  groups    = {Geometric Mechanics},
  journal   = {Mathematics of Control, Signals, and Systems},
  keywords  = {port-{H}amiltonian system, Descriptor system, Differential-algebraic equation, Passivity, Stability, System transformation, Differentiation index, Strangeness-index, Skew-adjoint operator},
  month     = oct,
  number    = {4},
  publisher = {Springer Science and Business Media {LLC}},
  title     = {Linear port-{H}amiltonian descriptor systems},
  volume    = {30},
  year      = {2018}
}

@article{Benner2019,
  abstract      = {In this paper, we study the identification problem of a passive system from tangential interpolation data. We present a simple construction approach based on the Mayo-Antoulas generalized realization theory that automatically yields a port-{H}amiltonian realization for every strictly passive system with simple spectral zeros. Furthermore, we discuss the construction of a frequency-limited port-{H}amiltonian realization. We illustrate the proposed method by means of several examples.},
  archiveprefix = {arXiv},
  author        = {Benner, Peter and Goyal, Pawan and Dooren, Paul Van},
  date          = {2019-10-31},
  day           = {31},
